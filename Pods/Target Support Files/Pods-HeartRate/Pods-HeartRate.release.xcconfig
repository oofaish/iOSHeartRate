FRAMEWORK_SEARCH_PATHS = "$(PODS_ROOT)/Metawear-iOSAPI"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/MBProgressHUD" "${PODS_ROOT}/Headers/Public/Metawear-iOSAPI"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/MBProgressHUD" -isystem "${PODS_ROOT}/Headers/Public/Metawear-iOSAPI"
OTHER_LDFLAGS = -ObjC -l"Pods-HeartRate-MBProgressHUD" -l"Pods-HeartRate-Metawear-iOSAPI" -framework "CoreGraphics" -framework "MetaWear"
OTHER_LIBTOOLFLAGS = $(OTHER_LDFLAGS)
PODS_ROOT = ${SRCROOT}/Pods